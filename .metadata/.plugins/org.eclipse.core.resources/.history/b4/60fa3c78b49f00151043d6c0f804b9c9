package sports_miniWiki;

public class NBALeaders_Individual {
	
	private int rank;
	private String name;
	private String market;
	private String team;
	private double stats;
	
	public NBALeaders_Individual(int rank, String name, String market, String team, double stats) {
		this.rank = rank;
		this.name = name;
		this.market = market;
		this.team = team;
		this.stats = stats;
	}
	
	public String toString() {
		String res = rank + ": " + name + " " + stats + " VS " + home;
		return res;
	}

}
